  "scripts": {
    "build": "npm run clean && npm run build-app",
    "build-app": "babel source -s -d dist --presets=@babel/env",
    "build-react": "react-scripts build",
    "clean": "rm -rf dist && mkdir dist",
    "codeClimate": "",
    "coveralls": "",
    "dev": "DEBUG=yourright:* npm run start-dev",
    "dev-server-client": "concurrently 'npm run dev' 'npm run start-react'",
    "eject-react": "react-scripts eject",
    "lint": "eslint \"src/**/*.js\" \"src/**/*.jsx\"",
    "lint:fix": "eslint --fix \"src/**/*.js\" \"src/**/*.jsx\"",
    "lint:server": "eslint \"app/source/**/*.js\"",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "pretest-server": "NODE_ENV=test npm run migrate:undo && NODE_ENV=test npm run migrate && NODE_ENV=test npm run seed",
    "release": "npm run clean && npm run build-app && npm run start",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "start": "node dist/www",
    "start-dev": "nodemon --exec babel-node source/bin/www.js",
    "start-react": "react-scripts start",
    "test-react": "react-scripts test",
    "test-server": "NODE_ENV=test jest tests/run.test.js --watchAll=false --detectOpenHandles"
  },
  "types": "build/index.d.ts",
  "version": "0.1.0"
}